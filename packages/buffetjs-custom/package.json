{
  "name": "@buffetjs/custom",
  "version": "1.0.0",
  "description": "Buffetjs Custom Components",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cross-env NODE_ENV=production rimraf dist && babel --config-file ../../.babelrc src --out-dir dist --copy-files --no-comments && rimraf dist/**/**/tests",
    "lint": "eslint --c ../../.eslintrc src",
    "prepublishOnly": "npm run build",
    "watch": "babel --config-file ../../.babelrc src --watch --out-dir dist --copy-files --no-comments && rimraf dist/**/**/tests"
  },
  "dependencies": {
    "@buffetjs/core": "^1.0.0",
    "@buffetjs/styles": "^1.0.0",
    "@buffetjs/utils": "^1.0.0",
    "moment": "^2.24.0",
    "react-moment-proptypes": "^1.7.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.0",
    "styled-components": "^4.2.1"
  },
  "peerDependencies": {
    "lodash": "^4.17.11",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.0",
    "styled-components": "^4.1.2"
  },
  "keywords": [
    "Buffetjs",
    "react",
    "components",
    "styled-components"
  ],
  "author": "Strapi team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
